{
  "name": "protozoom",
  "version": "1.0.0",
  "description": "Zooming prototype",
  "main": "main.js",
  "scripts": {
    "start": "npm run build && static",
    "build": "jsx src/ build/ && npm run autoprefix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch:js": "jsx --watch src/ build/",
    "watch:css": "onchange 'src/main.css' -- npm run autoprefix",
    "autoprefix": "autoprefixer src/main.css --output build/main.css"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gvn/protozoom.git"
  },
  "keywords": [
    "prototype"
  ],
  "author": "@gvn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gvn/protozoom/issues"
  },
  "homepage": "https://github.com/gvn/protozoom",
  "dependencies": {
    "node-static": "^0.7.6",
    "react": "^0.13.1",
    "react-draggable": "git://github.com/gvn/react-draggable#master"
  },
  "devDependencies": {
    "autoprefixer": "^5.1.0",
    "onchange": "^1.0.0",
    "react-tools": "^0.13.1"
  }
}
